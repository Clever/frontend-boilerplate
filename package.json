{
  "name": "frontend-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for frontend code at Clever",
  "scripts": {
    "dev-server": "webpack --watch & nodemon -e .ts --watch ./backend --watch ./ui --exec 'NODE_ENV=development PORT=3000 ./node_modules/.bin/ts-node --ignoreWarnings 2307 ./server.ts'; kill %1",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clever/frontend-boilerplate.git"
  },
  "keywords": [
    "frontend",
    "boilerplate"
  ],
  "author": "Omar Diab (osdiab) and Pam Martinez (ipam73)",
  "bugs": {
    "url": "https://github.com/Clever/frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/Clever/frontend-boilerplate#readme",
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "enzyme": "^2.8.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "redux-devtools-extension": "^2.13.0",
    "stylelint": "^7.9.0",
    "ts-jest": "^19.0.6",
    "tslint": "^5.0.0",
    "typescript-eslint-parser": "^2.0.0"
  },
  "dependencies": {
    "@types/node": "^7.0.12",
    "@types/react": "Clever/minimal-react-typings",
    "awesome-typescript-loader": "^3.1.2",
    "clever-components": "^0.23.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "fetch": "^1.1.0",
    "file-loader": "^0.10.1",
    "ignore-styles": "^5.0.1",
    "isomorphic": "0.0.11",
    "jsdom-global": "^2.1.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "nodemon": "^1.11.0",
    "react": "15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.16.1",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.17.1",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".+test.+(ts|tsx|js)",
    "setupFiles": [
      "./lib/jest/setup.js"
    ],
    "moduleNameMapper": {
      ".*(css|less|png|svg)$": "<rootDir>/lib/jest/staticImportStub.js"
    }
  }
}
